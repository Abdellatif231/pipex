cc -Wall -Wextra -Werror -c ft_puthex.c -o ft_puthex.o
cc -Wall -Wextra -Werror -c ft_printf.c -o ft_printf.o
ar rsc  libftprintf.a ft_putchar.o ft_putstr.o ft_putnbr.o ft_putunbr.o ft_putadr.o ft_puthex.o ft_printf.o
cd ./libft && make
